{
  "name": "rescript-form-starter",
  "version": "0.7.2",
  "description": "A starter kit for ReScript React applications",
  "main": "src/App.mjs",
  "repository": "git@github.com:idkjs/rescript-form-starter.git",
  "author": "idkjs <aarmand.inbox@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "postinstall": "esy",
    "bundle": "esbuild ./src/App.mjs --outfile=www/js/bundle.js --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --define:process.env.NODE_ENV='\"development\"'",
    "bundle:watch": "npm run bundle -- --watch --color=true",
    "dev-server": "PORT=5000 live-server ./www --host=localhost",
    "bs:build": "NINJA_ANSI_FORCED=1 esy x bsb -make-world",
    "bs:watch": "watchexec -w src --exts re,rei,ml,mli,atd,json -r -c 'npm run bs:build'",
    "bs:clean": "esy x bsb -clean-world",
    "start": "npm run clean; concurrently --names re,dev,js --kill-others 'npm run bs:watch' 'npm run dev-server' 'npm run bundle:watch'",
    "bundle:prod": "npm run bundle --minify --define:process.env.NODE_ENV='\"production\"'",
    "clean": "rm -rf www/js; npm run bs:clean",
    "starts": "rescript build -with-deps -w -ws 9999",
    "build": "rescript",
    "server": "node server/server.mjs",
    "test": "retest --with-dom test/*_test.mjs"
  },
  "devDependencies": {
    "bs-stdlib-shims": "idkjs/bs-stdlib-shims",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^9.0.1",
    "etag": "^1.8.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "mime": "^2.5.2",
    "rescript": "^9.1.4",
    "rescript-devserver-tools": "^1.0.16",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "concurrently": "^6.0.0",
    "esbuild": "^0.10.2",
    "esy": "*",
    "live-server": "^1.2.1"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@rescript/react": "^0.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "rescript-asyncdata": "^4.0.0",
    "rescript-future": "^2.0.0",
    "rescript-js": "^0.5.17",
    "rescript-request": "^3.0.1",
    "rescript-test": "^3.1.0"
  },
  "browser": {
    "fs": false,
    "tty": false,
    "constants": false,
    "child_process": false
  }
}
